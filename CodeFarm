P4325 Module
//input 10 numbers and output the number of their modules
#include<iostream>
using namespace std;
typedef struct {
    int data;
    int module;
}modu;
int num(modu a[])
{
    int i,j;
    int number= 0;
    for(i=0;i<10;i++)
    {
        for(j=i+1;j<10;j++)
        {
            if(a[j].module == a[i].module)
            {
                 number++;
                break;

            }
        }
    }
    return 10-number;
}
int main()
{

    int figure;
    modu arr[10];
    int i = 0;
    for(;i<10;i++)
    {
        cin>>arr[i].data;
        arr[i].module = arr[i].data%42;
    }

        figure = num(arr);


      cout<< figure;
      return 0;
}
//统计余数的个数并输出。关键是统计一个数组中不重复的元素的个数。这个sum是O(n^2)复杂度，可以改进为O（nlogn）。
